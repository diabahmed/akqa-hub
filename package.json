{
  "name": "akqa-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "graphql-codegen:generate": "graphql-codegen -r dotenv/config --config codegen.ts",
    "graphql-codegen:watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts",
    "setup": "./bin/setup.sh"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "src/**/*.{json,js,ts,jsx,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@contentful/f36-icons": "5.6.0",
    "@contentful/f36-tokens": "5.1.0",
    "@contentful/live-preview": "4.6.48",
    "@contentful/rich-text-react-renderer": "16.1.5",
    "@next/bundle-analyzer": "15.5.4",
    "dotenv": "17.2.3",
    "graphql": "16.11.0",
    "graphql-request": "^7.2.0",
    "graphql-tag": "^2.12.6",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-resources-to-backend": "1.2.1",
    "next": "15.5.4",
    "next-compose-plugins": "2.2.1",
    "next-i18n-router": "5.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-focus-lock": "2.13.6",
    "react-i18next": "16.0.0",
    "sharp": "0.34.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.4",
    "@contentful/rich-text-types": "17.2.4",
    "@eslint/eslintrc": "^3.3.1",
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/client-preset": "5.1.0",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript-graphql-request": "6.3.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/typography": "0.5.19",
    "@types/negotiator": "0.6.4",
    "@types/node": "24.7.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "husky": "9.1.7",
    "i18next": "25.6.0",
    "i18next-http-backend": "3.0.2",
    "lint-staged": "16.2.3",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "3.4.18",
